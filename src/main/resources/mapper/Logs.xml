<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.kion.kionhub.mapper.LogsMapper">
    <insert id="insertLog">
        INSERT INTO sys_logs(user_name,request_url,params,operation_description,operation_time)VALUES(#{username},#{requestUrl},#{params},#{operation_description},NOW());
    </insert>

    <select id="selectAll" resultType="cn.kion.kionhub.entity.Logs">
        SELECT * FROM sys_logs;
    </select>


</mapper>